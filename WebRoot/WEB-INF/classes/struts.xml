<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
      "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
      "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="action" extends="struts-default">
		<action name="search" class="com.action.UserAction" method="search">
			<result name="success">/searchSuccess.jsp</result>
		</action>
		<action name="regist" class="com.action.UserAction" method="regist">
			<result name="success">/registSuccess.jsp</result>
			<result name="error">/registError.jsp</result>
		</action>
		
	</package>
</struts>
